{
  "name": "react-pure",
  "version": "0.3.0",
  "description": "React components using Pure CSS",
  "author": "Paul Le Cam",
  "user": "hailocab",
  "main": "dist/index.js",
  "scripts": {
    "gh-pages": "TARGET=gh-pages webpack --config ./config",
    "deploy-gh-pages": "TARGET=gh-pages node ./config/deploy-gh-pages.js",
    "dist": "TARGET=dist webpack --config ./config && TARGET=dist-min webpack --config ./config",
    "clean": "rm -Rf ./dist",
    "lint": "eslint . --ext .js --ext .jsx",
    "compile": "npm run clean && babel --optional runtime --out-dir dist src",
    "watch": "babel --watch --optional runtime --out-dir dist src",
    "test": "jest --coverage ./dist && npm run lint",
    "start": "TARGET=dev node dev-server/server.js && npm run onchange",
    "onchange": "onchange 'dist/*.js' 'lib/**/*.js' -- jest ./dist",
    "prepublish": "babel ./src --out-dir ./dist",
    "version": "npm run test && npm run dist && npm version \"$@\" && npm run gh-pages && npm run deploy-gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hailocab/react-pure.git"
  },
  "keywords": [
    "react-component",
    "pure",
    "css"
  ],
  "license": "Apache 2",
  "bugs": {
    "url": "https://github.com/hailocab/react-pure/issues"
  },
  "homepage": "https://github.com/hailocab/react-pure",
  "dependencies": {
    "classnames": "^1.1.4"
  },
  "peerDependencies": {
    "react": ">=0.13.0 <1.0.0"
  },
  "devDependencies": {
    "babel": "^5.2.6",
    "babel-core": "^5.2.6",
    "babel-eslint": "^3.0.1",
    "babel-loader": "^5.0.0",
    "css-loader": "^0.12.0",
    "eslint": "^0.20.0",
    "eslint-loader": "^0.11.1",
    "eslint-plugin-react": "^2.2.0",
    "highlight-loader": "^0.3.0",
    "highlight.js": "^8.5.0",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.3.0",
    "jest-cli": "^0.4.0",
    "json-loader": "^0.5.1",
    "lodash": "^3.8.0",
    "markdown-loader": "^0.1.2",
    "onchange": "^1.1.0",
    "purecss": "^0.6.0",
    "react": "^0.13.0",
    "react-ghfork": "^0.3.0",
    "react-hot-loader": "^1.2.6",
    "style-loader": "^0.12.1",
    "webpack": "^1.8.11",
    "webpack-dev-server": "^1.8.2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/babel-runtime",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/react"
    ]
  }
}
